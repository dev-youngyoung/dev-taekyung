		<queryString>
			<![CDATA[select substr(b.vendcd,1,3)||'-'||substr(b.vendcd,4,2)||'-'||substr(b.vendcd,6,5) recv_vendcd
        ,b.member_name recv_member_name
        ,b.boss_name recv_boss_name
        ,b.address recv_address
        ,substr(c.vendcd,1,3)||'-'||substr(c.vendcd,4,2)||'-'||substr(c.vendcd,6,5) supp_vendcd
        ,c.member_name supp_member_name
        ,c.boss_name supp_boss_name
        ,c.address supp_address
        ,to_date(a.order_dt,'%Y%m%d') order_dt2
        ,(select user_name from tcb_person where member_no = a.recv_member_no and person_seq = a.order_person_seq) order_user_name
        ,(select user_name from tcb_person where member_no = a.recv_member_no and person_seq = a.check_person_seq) check_user_name
        ,a.*
from tcb_order_master a
      ,tcb_member b
      ,tcb_member c
where a.recv_member_no = b.member_no
  and a.supp_member_no = c.member_no
and a.recv_member_no = $P{recv_member_no}
and  a.order_no = $P{order_no}
and a.order_chasu = $P{order_chasu}]]>
		</queryString>
