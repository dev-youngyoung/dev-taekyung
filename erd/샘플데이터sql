function speSum(){
    var f = document.forms[''form1''];
    var cont_total_1 = 0;
    if(f[''a_pay_1''].value!="") cont_total_1 += Number(f[''a_pay_1''].value.replaceAll(",",""));
    if(f[''a_pay_2''].value!="") cont_total_1 += Number(f[''a_pay_2''].value.replaceAll(",",""));
