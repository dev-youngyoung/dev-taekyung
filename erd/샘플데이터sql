<script> 
function chgGubun(gubun) {
    var table = document.getElementById("gubun_table"); 
    var hide_area = getElementsByClass("sh_area", table);
    for (var i = 0; i < hide_area.length; i++) {
        hide_area[i].style.display = "none";
        var required = hide_area[i].getElementsByTagName("input");
        for(var j = 0 ; j < required.length ; j++){
            required[j].removeAttribute("required");
        }
    } 
    var show_area = getElementsByClass("sh_area_" + gubun, table);
    for (var i = 0; i < show_area.length; i++) {
        show_area[i].style.display = "";
        var required = show_area[i].getElementsByTagName("input");
        for(var j = 0 ; j < required.length ; j++){
            required[j].setAttribute("required","Y");
        }
    }
}
</script>


