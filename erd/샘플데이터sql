	}


}

// 특약사항 row 삭제
function delSpecialRow(tableId)
{
	var tableObj = document.getElementById(tableId);
		if(tableId == "tb_special"){
			if(tableObj.rows.length > 2){
				tableObj.deleteRow(tableObj.rows.length-1);
			}
		}else if(tableId == "tb_special3"){
			if(tableObj.rows.length > 2){
				tableObj.deleteRow(tableObj.rows.length-1);
			}
		}else if(tableId == "tb_special4"){
			if(tableObj.rows.length > 2){
				tableObj.deleteRow(tableObj.rows.length-1);
			}
		}else if(tableId == "tb_special5"){
			if(tableObj.rows.length > 2){
				tableObj.deleteRow(tableObj.rows.length-1);
			}
		}else{
			if(tableObj.rows.length > 3){
				tableObj.deleteRow(tableObj.rows.length-1);
			}
		}
}
