<script>
attachOnload(function (){
    if(location.href.indexOf(''contract_insert.jsp'')>0){
        //setWarr();
        
        var cont_total = document.getElementById("cont_total").value;
        if(document.getElementById("cont_total").value){
            replaceInput(cont_total, ''cont_give'', document.all.__html);
            fSetKoreanMoney(cont_total,''spn_cont_give'');
            chkWarr(cont_total);
        }
          if(document.getElementById("bid_method").value =="02"){
            document.forms[''form1''][''option_yn_3''].checked= true
        }
        
    }
});


function setWarrType(input,type,chk){
        var f = document.forms["form1"];
        var warr_row = document.getElementById("warr_grid").getElementsByTagName("tbody")[0];

        var warr_etc = "계약금액의 "+ document.getElementById("warr_rate").value +"%"+document.getElementById(input).innerText;

        var row_pos = -1;
        if(f[''warr_type'']){
            if(warr_row.rows.length >= 1){
                for(var i= 0; i < warr_row.rows.length; i++){
                    if(document.getElementsByName("warr_type")[i].value == type){
                        row_pos = i;
                        break;
                    }
                }
            }
        }

        if( row_pos > -1){// 행삭제
            warr_row.deleteRow(row_pos);
        }

        if(chk){
            // 행추가
            warr_grid.addRow();
            if(warr_row.rows.length > 1){
                f[''warr_type''][f[''warr_type''].length - 1].value = type;
                f[''warr_etc''][f[''warr_type''].length - 1].value = warr_etc;
            }else{
                f[''warr_type''].value = type;
                if(f[''warr_type''][0])f[''warr_type''][0].value = type;
                f[''warr_etc''].value = warr_etc;
                if(f[''warr_etc''][0])f[''warr_etc''][0].value = warr_etc;
            }
        }

    }



function setWarr(){
    if(document.getElementById("warr_text").innerText != ""){
        
        var warr_etc = "계약금액의 "+ document.getElementById("warr_rate").value +"%"+document.getElementById(input).innerText;
        document.forms[''temp_warr_form''].reset();
        SetFormValue("temp_warr_form","warr_type","10");
        SetFormValue("temp_warr_form","warr_etc", warr_etc );
        warr_grid.cells = table2array("warr_template");
        warr_grid.addRow();
    }
    
    document.forms[''temp_warr_form''].reset();
    warr_grid.cells = table2array("warr_template");
}



function chkWarr(val){
    
    var cont_total = 0;
    var isChk = false;
    var warr_text = "(이행보증증권 제출)";
    if(val!=  null){
        cont_total = Number(val.replaceAll(",",""));
    }
    
    // 계약금액이 2천만원 미만이면 각서 선택
    if(cont_total < 20000000){
        isChk = true;
        warr_text = "(지급각서로 대체)";
    }
    
    var agree_text = "";
    
    // 계약금액이 1천만원 미만이면 전결 
    if(cont_total < 10000000){
        agree_text = "전결"
    }
    document.getElementById("contract_agree_text").innerText = agree_text;
    
    document.forms[''form1''][''option_yn_2''].checked= isChk
    document.getElementById("warr_text").innerText = warr_text;
    setWarrType(''warr_text'',''10'', !isChk);
    chgWarr();
}


function chgWarr(){
    
    var cont_total = 0;
    var warr_rate = 0;
    var cont_warr = 0;
    if(document.getElementById("cont_total").value != null){
        cont_total = Number(document.getElementById("cont_total").value.replaceAll(",",""));
    }
    if(document.getElementById("warr_rate").value != null){
        warr_rate = Number(document.getElementById("warr_rate").value);
    }
    
    cont_warr = cont_total * warr_rate / 100 ;
    replaceInput(fnMakeComma2(cont_warr.toFixed(0).toString()), ''cont_warr'', document.all.__html);
    fSetKoreanMoney(cont_warr.toFixed(0).toString(),''spn_cont_warr'');
}
</script>
