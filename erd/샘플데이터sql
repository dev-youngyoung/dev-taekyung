function button_addRow2(tableId,rowCnt){
    var rowArr =getElementsByClass(rowCnt,document.getElementById(tableId),"tr");
    var rowLength = rowArr.length
    var tr = document.getElementById(tableId).getElementsByTagName("tbody")[0].rows[0].cloneNode(1);
    tr.getElementsByTagName("td")[0].innerText = rowLength+1;
    document.getElementById(tableId).getElementsByTagName("tbody")[0].appendChild(tr);
    
}

    function button_addRow(tableId){
        var tr = document.getElementById(tableId).getElementsByTagName("tbody")[0].rows[0].cloneNode(1);
        document.getElementById(tableId).getElementsByTagName("tbody")[0].appendChild(tr);
    }

    function button_delRow(tableId){
        var tableObj = document.getElementById(tableId).getElementsByTagName("tbody")[0];
        var rowCnt = tableObj.rows.length;
        if(rowCnt > 1){
            tableObj.deleteRow(rowCnt-1);
        }else{
            alert("더 이상 삭제 할 수 없습니다.");
        }
    }
