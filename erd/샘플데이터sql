<script>
function speSum(){
    var f = document.forms[''form1''];
    var cont_total = 0;
    if(f[''a_pay_1''].value!="") cont_total += Number(f[''a_pay_1''].value.replaceAll(",",""));
    if(f[''a_pay_2''].value!="") cont_total += Number(f[''a_pay_2''].value.replaceAll(",",""));
    if(f[''a_pay_3''].value!="") cont_total += Number(f[''a_pay_3''].value.replaceAll(",",""));
    if(f[''a_pay_4''].value!="") cont_total += Number(f[''a_pay_4''].value.replaceAll(",",""));
    if(f[''a_pay_5''].value!="") cont_total += Number(f[''a_pay_5''].value.replaceAll(",",""));
    if(f[''a_pay_6''].value!="") cont_total += Number(f[''a_pay_6''].value.replaceAll(",","")); 
    f[''cont_total''].value= fnMakeComma2(cont_total+"");
}
</script>
 
