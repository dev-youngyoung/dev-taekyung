<script>

function chgReson(obj){
    
    var objVal = obj.value;
    if(objVal=="②"){
        var chkReason = document.getElementsByClassName("chkReason");
        for (var i = 0; i < chkReason.length; i++) {
            chkReason[i].setAttribute("required","Y");
            chkReason[i].setAttribute("errfunc","tabView(3)");
            chkReason[i].setAttribute("chk_required","option_yn_3");
            chkReason[i].className = chkReason[i].className.replace(/input_n/gi,"input_r");
        }
    }else{
        var chkReason = document.getElementsByClassName("chkReason");
        for (var i = 0; i < chkReason.length; i++) {
            chkReason[i].removeAttribute("required");
            chkReason[i].removeAttribute("errfunc");
            chkReason[i].removeAttribute("chk_required");
            chkReason[i].className = chkReason[i].className.replace(/input_r/gi,"input_n");
        }
    }
}


function chgContTotal(tableId, totalId){
    
    var table = document.getElementById(tableId);
    var costArr = table.getElementsByClassName("cost");
    var total = 0;
    
     for (var i = 0; i < costArr.length; i++) {
        if(costArr[i]!=""){
            total += Number((costArr[i].value).replaceAll(",",""));
        }
    }  
     
     document.getElementById(totalId).value = fnMakeComma2(total.toString());
}
 
//   document.forms[''form1''][''option_yn_2''].checked= isChk
</script>

<table width="100%" border="0" cellpadding="0" cellspacing="0">
<tr><td style="font-size:18pt;font-weight:bold; " align="center">납품업체 종업원 파견 약정서</td></tr>
</table>
 

<p>
<table width="100%" border="0" cellpadding="0" cellspacing="0">
<colgroup><col></colgroup>
<tr>
    <td style="text-align:justify"><span class="cust_name_area_1">(주)농협충북유통</span>(이하"계약담당자"라 함)과  <span class="cust_name_area_2">____</span>(이하"계약상대자"라 함)는 "계약담당자"와 "계약상대자"가 체결한 기본 거래계약서 제 10조에 의거 납품업체 종업원 파견에 대하여 아래와 같이 약정한다.</td>
</tr>
