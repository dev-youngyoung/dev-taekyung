    // 생년월일
    function setBirth(index)
    {
        var f = document.forms[''form1''];
        if(f[''jumin_no''][index-1]==null)
        {
            alert("거래처를 먼저 선택하세요");
            f[''birthday1_''+index].value = "";
            f[''birthday2_''+index].value = "";
            return;
        }
        f[''jumin_no''][index-1].value = f[''birthday1_''+index].value + f[''birthday2_''+index].value
