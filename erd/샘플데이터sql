		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><textarea style="width:100%;height:20;overflow-y:hidden" class="input_r" name="t21" required="Y" hname="10조 조항내용"></textarea></span>'';
