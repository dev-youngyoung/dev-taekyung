<script type="text/javascript">
function button_addRow(tableId){
    var tr = document.getElementById(tableId).getElementsByTagName("tbody")[0].rows[0].cloneNode(1);
    document.getElementById(tableId).getElementsByTagName("tbody")[0].appendChild(tr);
}


function button_addRow2(tableId,bodyIdx,tdIdx,addText){
    
    var tableObj = document.getElementById(tableId).getElementsByTagName("tbody")[0];
    var sData = document.getElementById(tableId).getElementsByTagName("tbody")[bodyIdx].rows[0];
    var row_cnt = tableObj.rows.length;
    row = tableObj.insertRow(tableObj.rows.length);

    for(var i = 0; i < sData.cells.length; i++){
        var td = sData.cells[i];
        var cell = row.insertCell(i);
        var td_cont = td.innerHTML;
        if(i==tdIdx){
            td_cont = row_cnt+1+addText;
            row_cnt++;
        }
        cell.align = td.align;
        cell.innerHTML = td_cont;
    }
    var cost = document.getElementsByName("input_contract_cost");
    document.getElementById("totalCnt").innerText = cost.length;
}


function button_delRow(tableId, minRow){
    var tableObj = document.getElementById(tableId).getElementsByTagName("tbody")[0];
    var rowCnt = tableObj.rows.length;
    if(rowCnt > minRow){
        tableObj.deleteRow(rowCnt-1);
    }else{
        alert("더 이상 삭제 할 수 없습니다.");
    }
    var cost = document.getElementsByName("input_contract_cost");
    document.getElementById("totalCnt").innerText = cost.length;
}


function sumTotal(){
    
    var cost = document.getElementsByName("input_contract_cost");
    var cont_total = 0;
    var cont_sub_total = 0;
    var cont_middle_amt = 0;
    var cont_last_amt = 0;
    
    for(var i = 0;  i < cost.length ; i++ ){
        if(cost[i].value !=""){
            cont_total += Number(cost[i].value.replaceAll(",",""));
        }
    }
    
    document.getElementById("cont_total").value = fnMakeComma2(cont_total.toString());
    document.getElementById("totalCnt").innerText = cost.length;
    cont_sub_total = cont_total * 0.4;
    cont_middle_amt = cont_total * 0.3;
    document.getElementById("cont_sub_total").innerText = fnMakeComma2(cont_sub_total.toString());
    document.getElementById("cont_middle_amt").innerText = fnMakeComma2(cont_middle_amt.toString());
    document.getElementById("cont_last_amt").innerText = fnMakeComma2(cont_middle_amt.toString());
    fSetKoreanMoney(cont_total.toString() ,"spn_cont_total");
    fSetKoreanMoney(cont_sub_total.toString() ,"spn_cont_sub_total");
    fSetKoreanMoney(cont_middle_amt.toString() ,"spn_cont_middle_amt");
    fSetKoreanMoney(cont_middle_amt.toString() ,"spn_cont_last_amt");
}


</script>



<p>
<table  width="100%" align="center" cellspacing="0" cellpadding="0">
<tr>
    <td  align="right"><b>품의 No. : <span><input type="text" name="input_contract_no"   class="input_r" style="width: 180px;"   hname="품의번호" required="Y" errfunc="tabView(0)" /></span></b></td>
</tr>
