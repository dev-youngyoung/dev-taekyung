function addSpecialRow(tableId)
{
	var tableObj = document.getElementById(tableId);

	rowObj = tableObj.insertRow();              // tr 추가

	if(tableId == "tb_special"){
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><input type="text" name="t1" class="label" style="width:98%"></span>'';
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><input type="text" name="t2" class="label" style="width:98%"></span>'';
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><input type="text" name="t3" class="label" style="width:98%"></span>'';
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><input type="text" name="t4" class="label" style="width:98%"></span>'';
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = ''<span><input type="text" name="t5" class="label" style="width:98%"></span>'';
	}else if(tableId == "tb_special2"){
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.innerHTML = '''';
		cellObj = rowObj.insertCell();              // td 추가
		cellObj.vAlign="top";
		cellObj.innerHTML = ''<span><input type="text" class="label" style="width:98%"></span>'';
		cellObj = rowObj.insertCell();              // td 추가
